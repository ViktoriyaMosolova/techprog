<Window x:Class="lab3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="100,14,100,15">
            <TextBlock Width="200" Text="Введите начальный х:"/>
            <TextBox  Width="200"/>
            <TextBlock Width="200" Text="Введите конечный х:"/>
            <TextBox  Width="200"/>
            <TextBlock Width="45" Text="Вывод:" Margin="0,10,0,0"/>
            <TextBox Name="Text" Width="400" Height="250"/>
        </StackPanel>
    </Grid>
</Window>
